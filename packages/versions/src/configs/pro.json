{
  "version": "pro",
  "name": "专业版本",
  "description": "面向专业用户的增强版本",
  "theme": {
    "primaryColor": "#28a745",
    "backgroundColor": "#f8f9fa"
  },
  "features": {
    "user-management": {
      "name": "用户管理",
      "enabled": true,
      "version": "1.2.0",
      "icon": "👥",
      "description": "增强的用户管理功能，支持角色权限",
      "params": {
        "maxUsers": 1000,
        "allowBatchOperations": true,
        "roleManagement": true
      }
    },
    "basic-dashboard": {
      "name": "基础仪表板",
      "enabled": true,
      "version": "1.0.0",
      "icon": "📊",
      "description": "简单的数据展示面板",
      "params": {
        "refreshInterval": 60000,
        "maxCharts": 4
      }
    },
    "advanced-dashboard": {
      "name": "高级仪表板",
      "enabled": true,
      "version": "2.0.0",
      "icon": "📈",
      "description": "高级数据分析和可视化",
      "params": {
        "refreshInterval": 30000,
        "maxCharts": 8,
        "exportFormats": ["png", "pdf", "svg"],
        "realTimeUpdates": true
      }
    },
    "file-upload": {
      "name": "文件上传",
      "enabled": true,
      "version": "1.5.0",
      "icon": "📁",
      "description": "多文件上传，支持拖拽",
      "params": {
        "maxFileSize": "50MB",
        "allowedTypes": ["jpg", "png", "pdf", "doc", "xls"],
        "multipleFiles": true,
        "dragAndDrop": true
      }
    },
    "batch-operations": {
      "name": "批量操作",
      "enabled": true,
      "version": "1.5.0",
      "icon": "⚡",
      "description": "批量处理和操作功能",
      "params": {
        "maxBatchSize": 5000,
        "supportedOperations": ["import", "export", "delete", "update"],
        "progressTracking": true
      }
    },
    "data-export": {
      "name": "数据导出",
      "enabled": true,
      "version": "1.2.0",
      "icon": "📤",
      "description": "多格式数据导出功能",
      "params": {
        "formats": ["csv", "excel", "json", "pdf"],
        "maxRecords": 50000,
        "scheduledExport": true
      }
    }
  }
}
