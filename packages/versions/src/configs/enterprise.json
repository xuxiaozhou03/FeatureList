{
  "version": "enterprise",
  "name": "企业版本",
  "description": "面向大型企业的完整解决方案",
  "theme": {
    "primaryColor": "#6f42c1",
    "backgroundColor": "#f8f9fa"
  },
  "features": {
    "user-management": {
      "name": "用户管理",
      "enabled": true,
      "version": "2.0.0",
      "icon": "👥",
      "description": "企业级用户管理，支持SSO和LDAP集成",
      "params": {
        "maxUsers": "unlimited",
        "allowBatchOperations": true,
        "roleManagement": true,
        "ssoIntegration": true,
        "ldapSupport": true,
        "auditLog": true
      },
      "children": {
        "profile": {
          "name": "用户档案",
          "enabled": true,
          "version": "2.0.0",
          "icon": "👤",
          "description": "详细的用户档案管理",
          "params": {
            "customFields": true,
            "photoUpload": true
          },
          "children": {
            "settings": {
              "name": "个人设置",
              "enabled": true,
              "version": "2.0.0",
              "icon": "⚙️",
              "description": "个性化设置选项",
              "params": {
                "themeCustomization": true,
                "notificationPreferences": true
              }
            }
          }
        },
        "permissions": {
          "name": "权限管理",
          "enabled": true,
          "version": "2.0.0",
          "icon": "🔐",
          "description": "细粒度权限控制",
          "params": {
            "granularPermissions": true,
            "roleHierarchy": true
          }
        }
      }
    },
    "basic-dashboard": {
      "name": "基础仪表板",
      "enabled": true,
      "version": "1.0.0",
      "icon": "📊",
      "description": "简单的数据展示面板",
      "params": {
        "refreshInterval": 60000,
        "maxCharts": 4
      }
    },
    "advanced-dashboard": {
      "name": "高级仪表板",
      "enabled": true,
      "version": "3.0.0",
      "icon": "📈",
      "description": "企业级数据分析和可视化平台",
      "params": {
        "refreshInterval": 10000,
        "maxCharts": "unlimited",
        "exportFormats": ["png", "pdf", "svg", "ppt"],
        "realTimeUpdates": true,
        "customWidgets": true,
        "dataConnectors": ["sql", "api", "csv", "excel"],
        "alerting": true
      }
    },
    "file-upload": {
      "name": "文件上传",
      "enabled": true,
      "version": "2.0.0",
      "icon": "📁",
      "description": "企业级文件管理系统",
      "params": {
        "maxFileSize": "1GB",
        "allowedTypes": ["*"],
        "multipleFiles": true,
        "dragAndDrop": true,
        "virusScanning": true,
        "versionControl": true,
        "accessControl": true
      }
    },
    "batch-operations": {
      "name": "批量操作",
      "enabled": true,
      "version": "2.0.0",
      "icon": "⚡",
      "description": "企业级批量处理引擎",
      "params": {
        "maxBatchSize": "unlimited",
        "supportedOperations": [
          "import",
          "export",
          "delete",
          "update",
          "transform"
        ],
        "progressTracking": true,
        "errorHandling": true,
        "rollbackSupport": true,
        "scheduling": true
      }
    },
    "data-export": {
      "name": "数据导出",
      "enabled": true,
      "version": "2.0.0",
      "icon": "📤",
      "description": "企业级数据导出和同步",
      "params": {
        "formats": ["csv", "excel", "json", "pdf", "xml", "parquet"],
        "maxRecords": "unlimited",
        "scheduledExport": true,
        "dataSync": true,
        "encryption": true,
        "compressionFormats": ["zip", "gzip", "7z"]
      }
    },
    "api-integration": {
      "name": "API集成",
      "enabled": true,
      "version": "1.0.0",
      "icon": "🔗",
      "description": "第三方系统集成接口",
      "params": {
        "restApi": true,
        "graphqlApi": true,
        "webhooks": true,
        "rateLimiting": true,
        "apiDocumentation": true
      }
    },
    "enterprise-security": {
      "name": "企业安全",
      "enabled": true,
      "version": "1.0.0",
      "icon": "🛡️",
      "description": "企业级安全和合规功能",
      "params": {
        "dataEncryption": true,
        "accessAudit": true,
        "complianceReporting": true,
        "securityAlerts": true,
        "twoFactorAuth": true
      }
    }
  }
}
